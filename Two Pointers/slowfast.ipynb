    "def two_pointers_same_direction(arr):\n",
    "    slow = 0\n",
    "    for fast in range(len(arr)):\n",
    "        if arr[fast] != 0:   # condition to filter\n",
    "            arr[slow] = arr[fast]\n",
    "            slow += 1\n",
    "    return arr\n"
   ]
